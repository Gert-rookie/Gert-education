<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.fotg.eduservice.mapper.EduSubjectMapper">
    <insert id="saveEduSubject" parameterType="top.fotg.eduservice.entity.EduSubject">
            insert  into
            edu_subject
            (id,title,parent_id,sort,gmt_create,gmt_modified)
             values
             (#{id}, #{title}, #{parentId}, #{sort}, #{gmtCreate},#{gmtModified})
    </insert>
</mapper>
